{
    "name": "ejetar/laravel-formatter",
    "description": "A formatting library that converts data output between XML, CSV, JSON, TXT, YAML and a few others.",
    "homepage": "https://ejetar.com/projects/laravel-formatter",
    "version": "3.1.3",
    "readme": "README.md",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Guilherme Almeida Girardi",
            "email": "guilhermeagirardi@gmail.com",
            "homepage": "https://guilhermegirardi.com",
            "role": "Developer"
        }
    ],
    "keywords": [
        "php",
        "laravel",
        "format",
        "formatter",
        "output",
        "response",
        "data",
        "convert",
        "json",
        "csv",
        "xml",
        "yaml"
    ],
    "require": {
        "php" : ">=7.0.10",
        "league/csv": "~9.0",
        "mustangostang/spyc": "~0.6",
        "illuminate/support": "~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 || ^6.5"
    },
    "autoload": {
        "psr-4": {
            "Ejetar\\Formatter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Formatter": "Ejetar\\Formatter"
            }
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "test": "vendor\\bin\\phpunit"
    }
}